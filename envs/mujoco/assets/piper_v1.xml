<mujoco model="cheetah">
  <compiler angle="radian" coordinate="local" inertiafromgeom="true" settotalmass="14"/>
  <asset>
    <mesh name="torso" file="piper_stl/torso.stl" scale="1 1 1"/>
    <mesh name="back_left_thigh" file="piper_stl/BLTopLeg.stl" scale="1 1 1"/>
  </asset>
  <default>
    <joint armature=".1" damping=".01" limited="true" solimplimit="0 .8 .03" solreflimit=".02 1" stiffness="8"/>
    <geom conaffinity="0" condim="3" contype="1" friction=".4 .1 .1" rgba="0.8 0.6 .4 1" solimp="0.0 0.8 0.01" solref="0.02 1"/>
    <motor ctrllimited="true" ctrlrange="-1 1"/>
  </default>
  <size nstack="300000" nuser_geom="1"/>
  <option gravity="0 0 -9.81" timestep="0.01"/>
  <asset>
    <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
    <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01" rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
    <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
    <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="60 60" texture="texplane"/>
    <material name="geom" texture="texgeom" texuniform="true"/>
  </asset>
  <worldbody>
    <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3" specular=".1 .1 .1"/>
    <geom conaffinity="1" condim="3" material="MatPlane" name="floor" pos="0 0 0" rgba="0.8 0.9 0.8 1" size="40 40 40" type="plane"/>
    <body name="torso" pos="0 0 0.71">
      <geom pos="0 0 0" type="mesh" mesh="torso" rgba="0.8 0.6 0.2 1"/>
      <body name="back_left_thigh" pos="-150 0 0" axisangle="0 1 0 0">
        <joint pos="260 50 125" axis="0 1 0" damping="6" name="back_left_hip" range="-1.5 1.5" stiffness="80" type="hinge"/>
        <geom pos="0 0 0" type="mesh" mesh="back_left_thigh" rgba="0.2 0.8 0.2 1"/>
      </body>
    </body>
  </worldbody>
  <actuator>
    <motor gear="1000" joint="back_left_hip" name="back_left_hip"/>
  </actuator>
</mujoco>
